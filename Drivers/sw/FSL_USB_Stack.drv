%-BW_METHOD_BEGIN Init
%ifdef Init
%define! RetVal
%include Common\FSL_USB_StackInit.Inc
byte %'ModuleName'%.%Init(void)
{
  byte err;

%if (CPUDevice="MCF51MM256") | (CPUDevice="MCF51JE256")
  %'ModuleName'%.usb_int_dis();
%endif
%if (CPUDevice="MCF51JM128") | (CPUDevice="MC9S08JM128") | (CPUDevice="MC9S08JM32")
  SOPT2_USB_BIGEND = 0; /* Needs to be in little endian mode. */
%endif
  /* Initialize the USB interface */
%if USBDeviceClass="CDC Device"
  err = %@CDCDevice@'ModuleName'%.Init();
%elif USBDeviceClass="HID Keyboard Device"
  err = USB_Class_HID_Init(CONTROLLER_ID, USB_App_Callback, NULL, USB_App_Param_Callback);
%elif USBDeviceClass="MSD Host Device"
  err = %@MSDHost@'ModuleName'%.Init();
%else
  #error "unknown USBDeviceClass %USBDeviceClass"
%endif
  if(err != ERR_OK) {
    /* Error initializing USB Class */
%if defined(OnError)
    %OnError(%'ModuleName'%.USB_ERR_INIT);
%endif
    return ERR_FAILED;
  }
%if (CPUDevice="Kinetis K40") | (CPUDevice="Kinetis K53") | (CPUDevice="Kinetis K60")
  /* for Kinetis only */
  #if !HIGH_SPEED_DEVICE
    NVICICER2 |= (1<<9);        /* Clear any pending interrupts on USB */
    NVICISER2 |= (1<<9);        /* Enable interrupts from USB module */
  #else // HIGH_SPEED_DEVICE
    NVICICER3 |= 1;        /* Clear any pending interrupts on USBHS */
    NVICISER3 |= 1;        /* Enable interrupts on USBHS */
  #endif
%endif
%if (CPUDevice="MCF51MM256") | (CPUDevice="Kinetis K40") | (CPUDevice="Kinetis K53") | (CPUDevice="Kinetis K60") | (CPUDevice="MCF51JE256") | (CPUDevice="Kinetis L2K")
  %'ModuleName'%.usb_int_en();
%endif
  return ERR_OK;
}

