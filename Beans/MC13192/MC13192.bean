          <GrupItem>
            <TGrupItem>
              <Name>IRQ</Name>
              <Symbol>IRQgroup</Symbol>
              <Hint>This configures the interrupt request signal.</Hint>
              <ItemLevel>BASIC</ItemLevel>
              <BoldName>true</BoldName>
              <EditLine>false</EditLine>
              <Description/>
              <Expanded>Yes</Expanded>
              <Children>
                <GrupItem>
                  <TStrgListItem>
                    <Name>Macros</Name>
                    <Symbol>IRQMacrosList</Symbol>
                    <Hint>Required macros to configure and access IRQ settings</Hint>
                    <ItemLevel>BASIC</ItemLevel>
                    <EditLine>false</EditLine>
                    <DefaultValue>(string list)</DefaultValue>
                    <DefineList>false</DefineList>
                    <ExternEditorEnabled>false</ExternEditorEnabled>
                    <AcceptNonPrintChars>false</AcceptNonPrintChars>
                    <DefaultStrgList lines_count="37">
                      <Line>/* setting up the IRQ line from the modem:</Line>
                      <Line>* - pull up the signal</Line>
                      <Line>* - trigger on low level and/or falling edge</Line>
                      <Line>*/</Line>
                      <Line>#ifdef __HCS08__</Line>
                      <Line>/* Macros for the HCS08 core, using the IRQ pin */</Line>
                      <Line>#define IRQFLAG()         IRQSC_IRQF                                                    /*!&lt; poll IRQ flag */</Line>
                      <Line>#define IRQInit()       IRQSC = (IRQSC_IRQPE_MASK|IRQSC_IRQACK_MASK)                  /*!&lt; Configures IRQ: IRQ pin enable and acknowledge interrupt */</Line>
                      <Line>#define IRQACK()        IRQSC |= IRQSC_IRQACK_MASK                                    /*!&lt; IRQ acknowledge */</Line>
                      <Line>#define IRQPinEnable()  IRQSC = (IRQSC_IRQPE_MASK|IRQSC_IRQACK_MASK|IRQSC_IRQIE_MASK) /*!&lt; IRQ pin enable, acknowledge pending interrrupt and enable IRQ interrupt */</Line>
                      <Line>#else /* ColdFire */</Line>
                      <Line>#if defined(__GNUC__) /* Kinetis gcc? */</Line>
                      <Line>/* IRQ on PTD2 */</Line>
                      <Line>#define IRQINIT() \</Line>
                      <Line>  PORT_PDD_SetPinPullSelect(PORTD_BASE_PTR, 2, PORT_PDD_PULL_UP); \</Line>
                      <Line>  PORT_PDD_SetPinPullEnable(PORTD_BASE_PTR, 2, PORT_PDD_PULL_ENABLE);</Line>
                      <Line>#define IRQACK()  \</Line>
                      <Line>  PORT_PDD_ClearPinInterruptFlag(PORTD_BASE_PTR, ExtIntLdd1_PIN_INDEX) /* ack the interrupt on PORTA */</Line>
                      <Line>#elif 1</Line>
                      <Line>/* Macros for the MCF52259 using the IRQ on IRQ5 */</Line>
                      <Line>#define IRQFLAG()</Line>
                      <Line>#define IRQInit()</Line>
                      <Line>#define IRQACK()  (EPFR = IRQ1_PIN_MASK)</Line>
                      <Line>#define IRQPinEnable()</Line>
                      <Line>#else</Line>
                      <Line>/* Macros for the MCF51CN128 using the IRQ on PTE4 */</Line>
                      <Line>#define IRQFLAG()         KBI2SC_KBF       /*!&lt; poll IRQ flag */</Line>
                      <Line>#define IRQInit()       IRQ1_Disable();  /* disable interrupts */ \</Line>
                      <Line>KBI2ES_KBEDG4=0; /* falling edge, low level */ \</Line>
                      <Line>KBI2SC_KBIMOD=0; /* edge only */ \</Line>
                      <Line>PTEPE_PTEPE4=1;  /* pull up pin */ \</Line>
                      <Line>KBI2PE_KBIPE4=1; /* pin enable */ \</Line>
                      <Line>KBI2SC_KBACK=1   /* clear any false interrupts */</Line>
                      <Line>#define IRQACK()        KBI2SC_KBACK=1                         /*!&lt; IRQ acknowledge */</Line>
                      <Line>#define IRQPinEnable()  KBI2SC = (KBI2SC_KBACK_MASK|KBI2SC_KBIE) /*!&lt; Acknowledge any false interrupts and enable IRQ pin */</Line>
                      <Line>#endif</Line>
                      <Line>#endif</Line>
                    </DefaultStrgList>
                  </TStrgListItem>
                </GrupItem>
                <GrupItem>
                  <TInhrItem>
                    <Name>IRQ Pin</Name>
                    <Symbol>IRQ</Symbol>
                    <TypeSpec>MC13192\IRQ</TypeSpec>
                    <Hint>Interrupt signal from Transceiver</Hint>
                    <ItemLevel>BASIC</ItemLevel>
